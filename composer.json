{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-redis": "*",
        "api-platform/doctrine-orm": "^4.1.20",
        "api-platform/symfony": "^4.1.20",
        "bizkit/versioning-bundle": "^1.2.1",
        "doctrine/dbal": "^3.10|^4.3.1",
        "doctrine/doctrine-bundle": "^2.15.1",
        "doctrine/doctrine-migrations-bundle": "^3.4.2",
        "doctrine/orm": "^3.5.1",
        "friendsofsymfony/jsrouting-bundle": "^3.5.2",
        "inspector-apm/inspector-symfony": "^1.8.2",
        "jwage/phpamqplib-messenger": ">=0.8.5",
        "knplabs/knp-time-bundle": "^2.4",
        "nelmio/cors-bundle": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.6.2",
        "phpstan/phpdoc-parser": "^2.2.0",
        "survos/bootstrap-bundle": "^1.6.23",
        "survos/command-bundle": "^1.6.23",
        "survos/crawler-bundle": ">=1.6.23",
        "survos/meili-bundle": "^1.6.23",
        "survos/simple-datatables-bundle": "^1.6.23",
        "survos/workflow-helper-bundle": "^1.6.23",
        "symfony/amqp-messenger": "^7.3.2",
        "symfony/asset": "^7.3",
        "symfony/asset-mapper": "^7.3.2",
        "symfony/console": "^7.3.2",
        "symfony/doctrine-messenger": "^7.3.2",
        "symfony/dotenv": "^7.3.2",
        "symfony/expression-language": "^7.3.2",
        "symfony/flex": "^2.8.1",
        "symfony/form": "^7.3.2",
        "symfony/framework-bundle": "^7.3.2",
        "symfony/http-client": "^7.3.2",
        "symfony/intl": "^7.3.2",
        "symfony/mailer": "^7.3.2",
        "symfony/mime": "^7.3.2",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "^7.3",
        "symfony/process": "^7.3",
        "symfony/property-access": "^7.3.2",
        "symfony/property-info": "^7.3.1",
        "symfony/runtime": "^7.3.1",
        "symfony/security-bundle": "^7.3.2",
        "symfony/serializer": "^7.3.2",
        "symfony/stimulus-bundle": "^2.28.2",
        "symfony/string": "^7.3.2",
        "symfony/translation": "^7.3.2",
        "symfony/twig-bundle": "^7.3.2",
        "symfony/uid": "^7.3.1",
        "symfony/ux-chartjs": "^2.28.2",
        "symfony/ux-twig-component": ">=2.28.2",
        "symfony/validator": "^7.3.2",
        "symfony/web-link": "^7.3",
        "symfony/yaml": "^7.3.2",
        "twig/extra-bundle": "^2.12|^3.21",
        "twig/twig": "^2.12|^3.21.1",
        "zenstruck/class-metadata": "^1.1",
        "zenstruck/messenger-monitor-bundle": "^0.5.4"
    },
    "config": {
        "allow-plugins": {
            "endroid/installer": true,
            "php-http/discovery": true,
            "survos/installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "zenstruck/class-metadata": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR% --symlink --relative": "symfony-cmd",
            "fos:js-routing:dump --format=js --target=public/js/fos_js_routes.js --callback=\"export default \"": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.3"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1.22",
        "phpunit/phpunit": "^12.3.0",
        "pierstoval/smoke-testing": "^1.2.2",
        "survos/code-bundle": "^1.5.602",
        "symfony/browser-kit": "^7.3.2",
        "symfony/css-selector": "^7.3",
        "symfony/debug-bundle": "^7.3",
        "symfony/maker-bundle": "^1.64.0",
        "symfony/phpunit-bridge": "^7.3.1",
        "symfony/stopwatch": "^7.3.0",
        "symfony/web-profiler-bundle": "^7.3.2",
        "zenstruck/browser": "^1.9.1",
        "zenstruck/console-extra": "^1.4"
    }
}
